<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.mindyourlovedone.healthcare.Prescription.Prescription_detail_Activity">


    <RelativeLayout
        android:id="@+id/headers"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#ADAEB3"
        android:gravity="center_vertical"
        android:padding="@dimen/dimension3dp"
        android:visibility="visible">

        <ImageView
            android:id="@+id/imgBacksss"
            android:layout_width="@dimen/dimension40dp"
            android:layout_height="@dimen/dimension50dp"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:src="@drawable/backward_arrow" />

        <TextView
            android:id="@+id/txt_Titles"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="@dimen/dimension20dp"
            android:layout_marginTop="@dimen/dimension5dp"
            android:fontFamily="@font/lato"
            android:gravity="center_horizontal"
            android:text="Add Prescription"
            android:textColor="@color/colorWhite"
            android:textSize="@dimen/_18ssp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/txtSave"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dimension5dp"
            android:layout_marginRight="@dimen/dimension10dp"
            android:fontFamily="@font/lato"
            android:paddingLeft="@dimen/dimension5dp"
            android:paddingRight="@dimen/dimension5dp"
            android:text="Save"
            android:textColor="@color/colorWhite"
            android:textSize="@dimen/_18ssp"
            android:textStyle="normal" />

        <ImageView
            android:id="@+id/imgDones"
            android:layout_width="@dimen/dimension30dp"
            android:layout_height="@dimen/dimension30dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/dimension15dp"
            android:src="@drawable/check_mark"
            android:visibility="gone" />

        <ImageView
            android:id="@+id/imgDots"
            android:layout_width="@dimen/dimension30dp"
            android:layout_height="@dimen/dimension30dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/dimension15dp"
            android:src="@drawable/info_white"
            android:visibility="gone" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/username_dis"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/headers"
        android:background="#949498">

        <TextView
            android:id="@+id/tv_UserNane"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginStart="120dp"
            android:layout_marginLeft="120dp"
            android:layout_marginTop="@dimen/dimension8dp"
            android:layout_marginEnd="120dp"
            android:layout_marginRight="120dp"
            android:layout_marginBottom="@dimen/dimension8dp"
            android:fontFamily="@font/regular_lato"
            android:gravity="center"
            android:maxLines="1"
            android:text="Mr. ___________"
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textSize="16.8sp"
            android:textStyle="bold" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/table_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/username_dis">

        <LinearLayout
            android:id="@+id/linearview1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/archive_Text"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dimension15dp"
                android:layout_marginBottom="@dimen/dimension8dp"
                android:fontFamily="@font/bold_lato"
                android:gravity="center"
                android:maxLines="1"
                android:text="This prescription is archived"
                android:textAlignment="center"
                android:textColor="#63646A"
                android:textSize="@dimen/_16ssp"
                android:textStyle="bold"
                android:visibility="gone" />

            <TextView
                android:id="@+id/archive_Date"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dimension2dp"
                android:layout_marginBottom="@dimen/dimension8dp"
                android:fontFamily="@font/lato_bold"
                android:gravity="center"
                android:maxLines="1"
                android:text="Mar 15, 2020"
                android:textAlignment="center"
                android:textColor="#68696F"
                android:textSize="16.8sp"
                android:textStyle="bold"
                android:visibility="gone" />

            <ScrollView
                android:id="@+id/scroll_Content"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="#F2F2F7">

                <TableLayout
                    android:id="@+id/table_layout_1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dimension12dp"
                    android:layout_marginTop="@dimen/dimension12dp"
                    android:layout_marginRight="@dimen/dimension12dp"
                    android:layout_marginBottom="@dimen/dimension12dp"
                    android:background="#F2F2F7"
                    android:isScrollContainer="true"
                    android:orientation="vertical"
                    android:shrinkColumns="*"
                    android:stretchColumns="*">

                    <TableRow
                        android:id="@+id/taleRow_1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/table_bg"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:padding="@dimen/dimension16dp">

                            <LinearLayout
                                android:id="@+id/linear_RXName"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/dimension2dp"
                                    android:fontFamily="@font/regular_lato"
                                    android:text="RX Name"
                                    android:textColor="#8B8B8B"
                                    android:textSize="@dimen/_14ssp" />

                                <EditText
                                    android:id="@+id/etv_RXName"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/edittext_custom_bg"
                                    android:fontFamily="@font/regular_lato"
                                    android:gravity="left"
                                    android:inputType="text"
                                    android:maxLines="1"
                                    android:padding="@dimen/dimension12dp"
                                    android:text=""
                                    android:textAlignment="gravity"
                                    android:textColor="#404041"
                                    android:textSize="@dimen/_14ssp" />
                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/linear_DFU"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/dimension5dp"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/dimension2dp"
                                    android:fontFamily="@font/regular_lato"
                                    android:text="Directions for use"
                                    android:textColor="#8B8B8B"
                                    android:textSize="@dimen/_14ssp" />

                                <EditText
                                    android:id="@+id/edt_DOU"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/edittext_custom_bg"
                                    android:fontFamily="@font/regular_lato"
                                    android:gravity="left"
                                    android:inputType="text"
                                    android:maxLines="1"
                                    android:padding="@dimen/dimension12dp"
                                    android:text=""
                                    android:textAlignment="gravity"
                                    android:textColor="#404041"
                                    android:textSize="@dimen/_14ssp" />
                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/linear_Ferquency"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/dimension5dp"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/dimension2dp"
                                    android:fontFamily="@font/regular_lato"
                                    android:text="Frequency"
                                    android:textColor="#8B8B8B"
                                    android:textSize="@dimen/_14ssp" />

                                <EditText
                                    android:id="@+id/edt_Feq"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/edittext_custom_bg"
                                    android:fontFamily="@font/regular_lato"
                                    android:gravity="left"
                                    android:inputType="text"
                                    android:maxLines="1"
                                    android:padding="@dimen/dimension12dp"
                                    android:text=""
                                    android:textAlignment="gravity"
                                    android:textColor="#404041"
                                    android:textSize="@dimen/_14ssp" />
                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/linear_Dosage"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/dimension5dp"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/dimension2dp"
                                    android:fontFamily="@font/regular_lato"
                                    android:text="Dosage"
                                    android:textColor="#8B8B8B"
                                    android:textSize="@dimen/_14ssp" />

                                <EditText
                                    android:id="@+id/edt_Dose"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/edittext_custom_bg"
                                    android:fontFamily="@font/regular_lato"
                                    android:gravity="left"
                                    android:inputType="text"
                                    android:maxLines="1"
                                    android:padding="@dimen/dimension12dp"
                                    android:text=""
                                    android:textAlignment="gravity"
                                    android:textColor="#404041"
                                    android:textSize="@dimen/_14ssp" />
                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/linear_Malady"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/dimension5dp"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/dimension2dp"
                                    android:fontFamily="@font/regular_lato"
                                    android:text="Treatment for (Malady)"
                                    android:textColor="#8B8B8B"
                                    android:textSize="@dimen/_14ssp" />

                                <EditText
                                    android:id="@+id/edt_Malady"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/edittext_custom_bg"
                                    android:fontFamily="@font/regular_lato"
                                    android:gravity="left"
                                    android:inputType="text"
                                    android:maxLines="1"
                                    android:padding="@dimen/dimension12dp"
                                    android:text=""
                                    android:textAlignment="gravity"
                                    android:textColor="#404041"
                                    android:textSize="@dimen/_14ssp" />
                            </LinearLayout>

                        </LinearLayout>
                    </TableRow>

                    <TableRow
                        android:id="@+id/taleRow_2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dimension12dp"
                        android:background="@drawable/table_bg"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:padding="@dimen/dimension16dp">

                            <LinearLayout
                                android:id="@+id/linear_Doctor"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/dimension5dp"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/dimension2dp"
                                    android:fontFamily="@font/regular_lato"
                                    android:text="Prescribing Doctor"
                                    android:textColor="#8B8B8B"
                                    android:textSize="@dimen/_14ssp" />

                                <EditText
                                    android:id="@+id/edt_Doctor"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/edittext_custom_bg"
                                    android:fontFamily="@font/regular_lato"
                                    android:gravity="left"
                                    android:inputType="text"
                                    android:maxLines="1"
                                    android:padding="@dimen/dimension12dp"
                                    android:text=""
                                    android:textAlignment="gravity"
                                    android:textColor="#404041"
                                    android:textSize="@dimen/_14ssp"
                                    android:textStyle="normal" />
                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/linear_DateTime"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/dimension5dp"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/dimension2dp"
                                    android:fontFamily="@font/regular_lato"
                                    android:text="Date of Prescription"
                                    android:textColor="#8B8B8B"
                                    android:textSize="@dimen/_14ssp" />

                                <EditText
                                    android:id="@+id/edt_Date"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/edittext_custom_bg"
                                    android:fontFamily="@font/regular_lato"
                                    android:gravity="left"
                                    android:inputType="text"
                                    android:maxLines="1"
                                    android:padding="@dimen/dimension12dp"
                                    android:text=""
                                    android:textAlignment="gravity"
                                    android:textColor="#404041"
                                    android:textSize="@dimen/_14ssp"
                                    android:textStyle="normal" />
                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/linear_PharmacyName"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/dimension5dp"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/dimension2dp"
                                    android:fontFamily="@font/regular_lato"
                                    android:text="Name of Pharmacy"
                                    android:textColor="#8B8B8B"
                                    android:textSize="@dimen/_14ssp" />

                                <EditText
                                    android:id="@+id/edt_Pharmacy"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/edittext_custom_bg"
                                    android:fontFamily="@font/regular_lato"
                                    android:gravity="left"
                                    android:inputType="text"
                                    android:maxLines="1"
                                    android:padding="@dimen/dimension12dp"
                                    android:text=""
                                    android:textAlignment="gravity"
                                    android:textColor="#404041"
                                    android:textSize="@dimen/_14ssp" />
                            </LinearLayout>

                        </LinearLayout>
                    </TableRow>

                    <TableRow
                        android:id="@+id/taleRow_3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dimension12dp"
                        android:background="@drawable/table_bg"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/dimension15dp"
                            android:orientation="horizontal"
                            android:weightSum="1">

                            <TextView
                                android:layout_width="@dimen/dimension0dp"
                                android:layout_height="match_parent"
                                android:layout_gravity="left"
                                android:layout_weight="0.8"
                                android:fontFamily="@font/regular_lato"
                                android:gravity="center_vertical"
                                android:text="Over-the-counter (OTC)"
                                android:textAlignment="gravity"
                                android:textColor="#404041"
                                android:textSize="@dimen/_14ssp"
                                android:textStyle="normal" />

                            <Switch
                                android:id="@+id/switch1"
                                style="@style/Switch"
                                android:layout_width="@dimen/dimension0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="0.2" />

                        </LinearLayout>

                    </TableRow>

                    <TableRow
                        android:id="@+id/taleRow_4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dimension12dp"
                        android:background="@drawable/table_bg"
                        android:orientation="vertical">

                        <LinearLayout
                            android:id="@+id/linear_Note"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/dimension15dp"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="left"
                                android:layout_marginBottom="@dimen/dimension8dp"
                                android:fontFamily="@font/regular_lato"
                                android:gravity="left"
                                android:text="Prescription Notes"
                                android:textAlignment="gravity"
                                android:textColor="#8B8B8B"
                                android:textSize="@dimen/_14ssp" />

                            <EditText
                                android:id="@+id/edt_Notes"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:background="@drawable/edittext_custom_bg"
                                android:contentDescription="Enter Input Here"
                                android:editable="true"
                                android:fontFamily="@font/lato"
                                android:gravity="top|left"
                                android:hint=" Short ''Note'' to be remembered"
                                android:imeOptions="actionDone"
                                android:inputType="textMultiLine"
                                android:lines="3"
                                android:maxLines="20"
                                android:padding="@dimen/dimension12dp"
                                android:scrollbars="vertical"
                                android:text=""
                                android:textColor="#404041"
                                android:textSize="@dimen/_14ssp" />

                        </LinearLayout>
                    </TableRow>

                    <TableRow
                        android:id="@+id/taleRow_5"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dimension12dp"
                        android:background="@drawable/table_bg"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/dimension15dp"
                            android:orientation="vertical">

                            <LinearLayout
                                android:id="@+id/linear_PrescriptionImg"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/dimension5dp"
                                android:orientation="horizontal"
                                android:weightSum="1">

                                <TextView
                                    android:id="@+id/tv_AddPic"
                                    android:layout_width="@dimen/dimension0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_vertical"
                                    android:layout_marginBottom="@dimen/dimension8dp"
                                    android:layout_weight="0.8"
                                    android:fontFamily="@font/regular_lato"
                                    android:gravity="left"
                                    android:text="Pictures of prescription"
                                    android:textAlignment="gravity"
                                    android:textColor="#8B8B8B"
                                    android:textSize="@dimen/_14ssp" />

                                <ImageView
                                    android:id="@+id/imgAddPic"
                                    android:layout_width="@dimen/dimension0dp"
                                    android:layout_height="@dimen/dimension40dp"
                                    android:layout_marginLeft="@dimen/dimension2dp"
                                    android:layout_weight="0.2"
                                    android:src="@drawable/plusnew" />
                            </LinearLayout>


                            <HorizontalScrollView
                                android:id="@+id/scroll_PrescriptionImg"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:scrollbars="none"
                                android:visibility="gone">

                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal">

                                    <LinearLayout
                                        android:id="@+id/linear_ListPrescImg"
                                        android:layout_width="@dimen/dimension165dp"
                                        android:layout_height="wrap_content"
                                        android:layout_marginRight="@dimen/dimension18dp"
                                        android:orientation="vertical">

                                        <ImageView
                                            android:id="@+id/img_PrspImg"
                                            android:layout_width="@dimen/dimension165dp"
                                            android:layout_height="@dimen/dimension200dp"
                                            android:background="@drawable/table_bg"
                                            android:padding="@dimen/dimension5dp" />

                                        <TextView
                                            android:id="@+id/tv_PrspImgDate"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_gravity="center"
                                            android:fontFamily="@font/lato"
                                            android:gravity="center"
                                            android:text=""
                                            android:textAlignment="gravity"
                                            android:textColor="#485357"
                                            android:textSize="@dimen/_12ssp"
                                            android:textStyle="normal" />
                                    </LinearLayout>
                                    <!-- <LinearLayout
                                         android:layout_width="@dimen/dimension165dp"
                                         android:layout_height="wrap_content"
                                         android:orientation="vertical"
                                         android:weightSum="2"
                                         android:layout_marginRight="@dimen/dimension18dp">
                                         <ImageView
                                             android:layout_width="@dimen/dimension165dp"
                                             android:layout_height="@dimen/dimension200dp"
                                             android:padding="@dimen/dimension8dp"
                                             android:src="@drawable/pr_img"
                                             android:background="@drawable/table_bg"/>
                                         <TextView
                                             android:layout_width="wrap_content"
                                             android:layout_height="wrap_content"
                                             android:text="Sep 16, 2020"
                                             android:textColor="#485357"
                                             android:textSize="@dimen/_12ssp"
                                             android:fontFamily="@font/lato"
                                             android:layout_gravity="center"
                                             android:gravity="center"
                                             android:textAlignment="gravity"
                                             android:textStyle="normal"/>
                                     </LinearLayout>
                                     <LinearLayout
                                         android:layout_width="@dimen/dimension165dp"
                                         android:layout_height="wrap_content"
                                         android:orientation="vertical"
                                         android:layout_marginRight="@dimen/dimension18dp">
                                         <ImageView
                                             android:layout_width="@dimen/dimension165dp"
                                             android:layout_height="@dimen/dimension200dp"
                                             android:src="@drawable/pre"
                                             android:padding="@dimen/dimension5dp"
                                             android:background="@drawable/table_bg"/>
                                         <TextView
                                             android:layout_width="wrap_content"
                                             android:layout_height="wrap_content"
                                             android:text="Sep 16, 2020"
                                             android:textColor="#485357"
                                             android:textSize="@dimen/_12ssp"
                                             android:fontFamily="@font/lato"
                                             android:layout_gravity="center"
                                             android:gravity="center"
                                             android:textAlignment="gravity"
                                             android:textStyle="normal"/>
                                     </LinearLayout>-->


                                </LinearLayout>
                            </HorizontalScrollView>
                        </LinearLayout>
                    </TableRow>

                    <TableRow
                        android:id="@+id/taleRow_6"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dimension20dp"
                        android:background="#F2F2F7"
                        android:orientation="vertical">

                        <LinearLayout
                            android:id="@+id/linear_BtnACD"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <LinearLayout
                                android:id="@+id/linear_archive"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/dimension60dp"
                                android:layout_marginTop="@dimen/dimension10dp"
                                android:background="@drawable/bg_green"
                                android:orientation="horizontal"
                                android:padding="@dimen/dimension16dp"
                                android:weightSum="2">

                                <ImageView
                                    android:layout_width="@dimen/dimension30dp"
                                    android:layout_height="@dimen/dimension20dp"
                                    android:layout_gravity="center"
                                    android:src="@drawable/tick" />

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_vertical"
                                    android:layout_marginLeft="@dimen/dimension12dp"
                                    android:fontFamily="@font/lato"
                                    android:text="Archive The Prescription"
                                    android:textAlignment="gravity"
                                    android:textColor="@android:color/white"
                                    android:textSize="@dimen/_14ssp"
