                    android:layout_alignParentBottom="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginRight="@dimen/dimension10dp"
                    android:layout_marginBottom="@dimen/dimension20dp"
                    android:gravity="center_horizontal"
                    android:text="Login from Dropbox"
                    android:textColor="@color/colorBlue"
                    android:textSize="@dimen/dimension18dp" />

                <TextView
                    android:id="@+id/txtLogoutDropbox"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/txtLoginDropbox"
                    android:layout_centerHorizontal="true"
                    android:layout_marginRight="@dimen/dimension10dp"
                    android:layout_marginBottom="@dimen/dimension20dp"
                    android:fontFamily="@font/lato"
                    android:gravity="center_horizontal"
                    android:text="Login as a different Dropbox user"
                    android:textColor="@color/colorBlue"
                    android:textSize="@dimen/dimension18dp"
                    android:visibility="gone" />

            </LinearLayout>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rlBackup"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_above="@+id/llBox"
            android:layout_alignStart="@+id/llBox"
            android:layout_alignLeft="@+id/llBox"
            android:visibility="gone"></RelativeLayout>

    </LinearLayout>

