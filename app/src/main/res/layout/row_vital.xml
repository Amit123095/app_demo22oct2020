<?xml version="1.0" encoding="utf-8"?>
<com.mindyourlovedone.healthcare.SwipeCode.SwipeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/swipe"
    android:layout_marginTop="@dimen/dimension10dp"
    android:layout_marginLeft="@dimen/dimension10dp"
    android:layout_marginRight="@dimen/dimension10dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <LinearLayout
        android:id="@+id/linswipe"
        android:layout_width="@dimen/dimension160dp"
        android:layout_height="match_parent"
        android:gravity="center"
        android:tag="Bottom3"
        android:weightSum="1">

        <LinearLayout
            android:id="@+id/lincall"
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#C9C9CE"
            android:gravity="center"
            android:visibility="gone">

            <ImageView
                android:id="@+id/call"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:src="@drawable/ic_call" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/lintrash"
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#FF5534"
            android:gravity="center">

            <ImageView
                android:id="@+id/trash"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="center"
                android:src="@drawable/ic_delete" />
        </LinearLayout>

    </LinearLayout>
    <!-- xmlns:android="http://schemas.android.com/apk/res/android"
          -->
    <RelativeLayout
        android:background="@drawable/border_row_white_app"
        android:id="@+id/rlVital"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >

        <RelativeLayout
            android:id="@+id/rlMain"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:gravity="center">

            <ImageView
                android:id="@+id/imgDropDown"
                android:layout_width="@dimen/dimension25dp"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="false"
                android:layout_marginTop="@dimen/dimension15dp"
                android:layout_marginRight="@dimen/dimension15dp"
                android:padding="@dimen/dimension5dp"
                android:src="@drawable/drop_down" />

            <RelativeLayout
                android:id="@+id/rlBP"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dimension10dp">

                <TextView
                    android:fontFamily="@font/lato"
                    android:id="@+id/txtBP"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_marginTop="@dimen/dimension10dp"
                    android:gravity="center"
                    android:padding="@dimen/dimension5dp"
                    android:text="BP"
                    android:textColor="@color/colorBlackDark"
                    android:textSize="@dimen/dimension16dp"
                    android:textStyle="bold"
                    android:visibility="visible" />

                <TextView
                    android:fontFamily="@font/lato"
                    android:id="@+id/txtBpValue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimension10dp"
                    android:layout_toRightOf="@+id/txtBP"
                    android:gravity="center"
                    android:padding="@dimen/dimension5dp"
                    android:text="100/60,"
                    android:textColor="@color/colorBlackDark"

                    android:textSize="@dimen/dimension16dp"
                    android:textStyle="bold"
                    android:visibility="visible" />

                <TextView
                    android:fontFamily="@font/lato"
                    android:id="@+id/txtHR"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dimension5dp"
                    android:layout_marginTop="@dimen/dimension10dp"
                    android:layout_toRightOf="@+id/txtBpValue"
                    android:gravity="center"
                    android:padding="@dimen/dimension5dp"
                    android:text="HR"
                    android:textColor="@color/colorBlackDark"

                    android:textSize="@dimen/dimension16dp"
                    android:textStyle="bold"
                    android:visibility="visible" />

                <TextView
                    android:fontFamily="@font/lato"
                    android:id="@+id/txtHRValue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimension10dp"
                    android:layout_toRightOf="@+id/txtHR"
                    android:gravity="center"
                    android:padding="@dimen/dimension5dp"
                    android:text="90,"
                    android:textColor="@color/colorBlackDark"
                    android:textSize="@dimen/dimension16dp"
                    android:textStyle="bold"
                    android:visibility="visible" />

                <TextView
                    android:fontFamily="@font/lato"
                    android:id="@+id/txtTemp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dimension5dp"
                    android:layout_marginTop="@dimen/dimension10dp"
                    android:layout_toRightOf="@+id/txtHRValue"
                    android:gravity="center"
                    android:padding="@dimen/dimension5dp"
                    android:text="T"
                    android:textColor="@color/colorBlackDark"
                    android:textSize="@dimen/dimension16dp"
                    android:textStyle="bold"
                    android:visibility="visible" />

                <TextView
                    android:fontFamily="@font/lato"
                    android:id="@+id/txtTempValue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimension10dp"
                    android:layout_toRightOf="@+id/txtTemp"
                    android:gravity="center"
                    android:padding="@dimen/dimension5dp"
                    android:text="96.5"
                    android:textColor="@color/colorBlackDark"
                    android:textSize="@dimen/dimension16dp"
                    android:textStyle="bold"
                    android:visibility="visible" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rlDateBP"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/rlBP"
                android:layout_marginLeft="@dimen/dimension10dp"
                android:gravity="center">

                <TextView
                    android:fontFamily="@font/lato"
                    android:id="@+id/txtDate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:padding="@dimen/dimension5dp"
                    android:text="18 Feb 2019"
                    android:textColor="@color/colorBlackDark"
                    android:textSize="@dimen/dimension16dp"
                    android:visibility="visible" />

                <View
                    android:id="@+id/view1"
                    android:layout_width="@dimen/dimension2dp"
                    android:layout_height="@dimen/dimension1dp"
                    android:layout_centerInParent="true"
                    android:layout_toRightOf="@+id/txtDate"
                    android:background="@color/colorBlackDark" />

                <TextView
                    android:fontFamily="@font/lato"
                    android:id="@+id/txtTime"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/view1"
                    android:gravity="center"
                    android:padding="@dimen/dimension5dp"
                    android:text="12:26 PM"
                    android:textColor="@color/colorBlackDark"
                    android:textSize="@dimen/dimension16dp"
                    android:visibility="visible" />

            </RelativeLayout>

            <LinearLayout
                android:id="@+id/llSubVital"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/rlDateBP"
                android:orientation="vertical"
                android:visibility="gone">

                <View
                    android:id="@+id/viewApp"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimension1dp"
                    android:layout_marginTop="@dimen/dimension5dp"
                    android:layout_marginBottom="@dimen/dimension5dp"
                    android:background="@color/colorOnesGray"
                    android:visibility="gone" />

                <!--Completion Date Commented-->
                <!--Date comment ends here-->


                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_below="@+id/llDate"
                    android:layout_marginLeft="@dimen/dimension10dp"
                    android:layout_marginRight="@dimen/dimension10dp"
                    android:background="@color/colorOnesGray"
                    android:visibility="visible"></View>

                <TextView
                    android:fontFamily="@font/lato"
                    android:id="@+id/txtPulse"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/view"
                    android:layout_marginLeft="@dimen/dimension10dp"
                    android:padding="@dimen/dimension5dp"
                    android:text="Pulse Rate"
                    android:textSize="@dimen/dimension16dp"
                    android:visibility="visible" />

                <TextView
                    android:fontFamily="@font/lato"
                    android:id="@+id/txtPulseData"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/txtNote"
                    android:layout_marginLeft="@dimen/dimension10dp"
                    android:paddingLeft="@dimen/dimension5dp"
                    android:text="Lasik Surgery Frequently Asked Questions"
                    android:textColor="@color/colorBlackDark"
                    android:visibility="visible" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_below="@+id/llDate"
                    android:layout_marginLeft="@dimen/dimension10dp"
                    android:layout_marginRight="@dimen/dimension10dp"
                    android:background="@color/colorOnesGray"
                    android:visibility="visible"></View>

                <TextView
                    android:fontFamily="@font/lato"
                    android:id="@+id/txtResp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/view"
                    android:layout_marginLeft="@dimen/dimension10dp"
                    android:padding="@dimen/dimension5dp"
                    android:text="Respiratory Rate"
                    android:textSize="@dimen/dimension16dp"
                    android:visibility="visible" />

                <TextView
                    android:fontFamily="@font/lato"
                    android:id="@+id/txtRespData"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/txtNote"
                    android:layout_marginLeft="@dimen/dimension10dp"
                    android:paddingLeft="@dimen/dimension5dp"
                    android:text="Lasik Surgery Frequently Asked Questions"
                    android:textColor="@color/colorBlackDark"
                    android:visibility="visible" />


                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_below="@+id/llDate"
                    android:layout_marginLeft="@dimen/dimension10dp"
                    android:layout_marginRight="@dimen/dimension10dp"
                    android:background="@color/colorOnesGray"
                    android:visibility="visible"></View>

                <TextView
                    android:fontFamily="@font/lato"
                    android:id="@+id/txtLoc"
                    style="@style/EmergencyDesc"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/view"
                    android:text="Location"
                    android:textSize="@dimen/dimension16dp"
                    android:layout_marginLeft="@dimen/dimension10dp"
                    android:padding="@dimen/dimension5dp"
                    android:visibility="visible" />

                <TextView
                    android:fontFamily="@font/lato"
                    android:id="@+id/txtLocData"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/txtNote"
                    android:layout_marginLeft="@dimen/dimension10dp"
                    android:paddingLeft="@dimen/dimension5dp"
                    android:text="Lasik Surgery Frequently Asked Questions"
                    android:textColor="@color/colorBlackDark"
                    android:visibility="visible" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_below="@+id/llDate"
                    android:layout_marginLeft="@dimen/dimension10dp"
                    android:layout_marginRight="@dimen/dimension10dp"
                    android:background="@color/colorOnesGray"
                    android:visibility="visible"></View>
                <TextView
                    android:fontFamily="@font/lato"
                    android:id="@+id/tilCol"
                    style="@style/EmergencyDesc"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/view"
                    android:text="Cholesterol"
                    android:textSize="@dimen/dimension16dp"
                    android:layout_marginLeft="@dimen/dimension10dp"
                    android:padding="@dimen/dimension5dp"
                    android:visibility="visible" />

                <TextView
                    android:fontFamily="@font/lato"
                    android:id="@+id/txtCol"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tilCol"
                    android:layout_marginLeft="@dimen/dimension10dp"
                    android:paddingLeft="@dimen/dimension5dp"
                    android:text="Lasik Surgery Frequently Asked Questions"
                    android:textColor="@color/colorBlackDark"
                    android:visibility="visible" />

                <View
                    android:id="@+id/viewCol"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_below="@+id/txtCol"
                    android:layout_marginLeft="@dimen/dimension10dp"
                    android:layout_marginTop="@dimen/dimension5dp"
                    android:layout_marginRight="@dimen/dimension10dp"
                    android:layout_marginBottom="@dimen/dimension10dp"
                    android:background="@color/colorOnesGray"
                    android:visibility="visible"></View>
                <TextView
                    android:fontFamily="@font/lato"
                    android:id="@+id/tilOter"
                    style="@style/EmergencyDesc"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/viewCol"
                    android:text="Other"
                    android:textSize="@dimen/dimension16dp"
                    android:layout_marginLeft="@dimen/dimension10dp"
                    android:padding="@dimen/dimension5dp"
                    android:visibility="visible" />

                <TextView
                    android:fontFamily="@font/lato"
                    android:id="@+id/txtOter"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tilOter"
                    android:layout_marginLeft="@dimen/dimension10dp"
                    android:paddingLeft="@dimen/dimension5dp"
                    android:text="Lasik Surgery Frequently Asked Questions"
                    android:textColor="@color/colorBlackDark"
                    android:visibility="visible" />

                <View
                    android:id="@+id/viewOter"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_below="@+id/txtOter"
                    android:layout_marginLeft="@dimen/dimension10dp"
                    android:layout_marginTop="@dimen/dimension5dp"
                    android:layout_marginRight="@dimen/dimension10dp"
                    android:layout_marginBottom="@dimen/dimension10dp"
                    android:background="@color/colorOnesGray"
                    android:visibility="visible"></View>
                <TextView
                    android:fontFamily="@font/lato"
                    android:id="@+id/txtNote"
                    style="@style/EmergencyDesc"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/viewOter"
                    android:text="Note"
                    android:textSize="@dimen/dimension16dp"
                    android:layout_marginLeft="@dimen/dimension10dp"
                    android:padding="@dimen/dimension5dp"
                    android:visibility="visible" />

                <TextView
                    android:fontFamily="@font/lato"
                    android:id="@+id/txtNoteData"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/txtNote"
                    android:layout_marginLeft="@dimen/dimension10dp"
                    android:paddingLeft="@dimen/dimension5dp"
                    android:text="Lasik Surgery Frequently Asked Questions"
                    android:textColor="@color/colorBlackDark"
                    android:visibility="visible" />

                <View
                    android:id="@+id/viewNote"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_below="@+id/txtNoteData"
                    android:layout_marginLeft="@dimen/dimension10dp"
                    android:layout_marginTop="@dimen/dimension5dp"
                    android:layout_marginRight="@dimen/dimension10dp"
                    android:layout_marginBottom="@dimen/dimension10dp"
                    android:background="@color/colorOnesGray"
                    android:visibility="visible"></View>
            </LinearLayout>

        </RelativeLayout>


        <View
            android:id="@+id/viewVital"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimension2dp"
            android:layout_below="@+id/rlMain"
            android:background="@color/colorOnesGray"
            android:visibility="visible"></View>

        <View
            android:id="@+id/view2"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimension10dp"
            android:layout_below="@+id/rlMain"
            android:background="@drawable/border_row_white_one"
            android:visibility="visible"></View>

        <TextView
            android:fontFamily="@font/lato"
            android:id="@+id/txtEdit"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/viewVital"
            android:background="@drawable/border_edit_note"
            android:gravity="center"
            android:padding="@dimen/dimension10dp"
            android:text="Edit Vital Signs"
            android:textAllCaps="true"
            android:textColor="@color/colorBlue"
            android:textSize="@dimen/dimension16dp"
            android:textStyle="bold"
            android:visibility="gone" />
    </RelativeLayout>
</com.mindyourlovedone.healthcare.SwipeCode.SwipeLayout>
